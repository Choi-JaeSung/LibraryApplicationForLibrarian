import java.util.TreeSet;

public class Library {
   private String name;
   private TreeSet<Borrower> borrowers;
   private TreeSet<Book> books;
   
   public Library(String name){
      this.name = name;
   }
   
   public boolean registerOneBorrower(String borrowerInformation){
      borrowers.add(new Borrower(borrowerInformation));
      return true;
   }
   
   public boolean addOneBook(String title,String author){
      books.add(new Book(books.size(),title,author));
      return true;
   }
}
